@model IEnumerable<EntityTaskModel.StudentDTO>;

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Course</th>
            <th>Age</th>
            <th>City</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.StudentId</td>
                <td>@item.StudentName</td>
                <td>@item.courses?.CourseName</td>
                <td>@item.Age</td>
                <td>@item.City</td>
            </tr>
        }
        <!-- Add more rows here if needed -->
    </tbody>
</table>